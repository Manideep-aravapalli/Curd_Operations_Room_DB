<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.curdoperationassignment">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CurdOperationAssignment">
        <activity
            android:name=".PopActivity"
            android:theme="@style/Theme.AppCompat.Dialog.Alert"></activity>
        <activity android:name=".UserList" />
        <activity
            android:name=".UpdateUserActivity"
            android:theme="@style/Theme.AppCompat.Dialog.Alert" />
        <activity android:name=".SaveUser" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>